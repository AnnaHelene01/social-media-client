{
  "name": "css-frameworks-ca",
  "version": "1.0.0",
  "description": "Ca - Workflow",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "build-vite": "npm run vite build",
    "prepare": "husky install",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.27.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "live-server": "^1.2.2",
    "prettier": "^2.7.1",
    "sass": "^1.54.8",
    "vite": "^3.2.3"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnnaHelene01/workflow-social-ca.git"
  },
  "bugs": {
    "url": "https://github.com/AnnaHelene01/workflow-social-ca/issues"
  },
  "homepage": "https://github.com/AnnaHelene01/workflow-social-ca#readme",
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix"
    ],
    "*.{html,js}": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
